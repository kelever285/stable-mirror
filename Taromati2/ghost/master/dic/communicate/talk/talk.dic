//**** 人格被動對話 *******************************************************************************
//---- OnCommunicate事件 ----------------------------------------------------------------
OnCommunicate{
	reference1=TOSIMPLIFIED(reference.raw[1])
	if reference0 == 'user' || reference0 == 'User'{
		//與user對話
		lastTalk = ReplyToUser
	}
	else{
		//與其他人格對話
		lastTalk = ReplyToGhost
	}
	LastTalkArr(lastTalk)
	lastTalk
}
LastTalkArr{
	_lasttalk=IARRAY
	_talk=_argv[0]
	lastTalkHistory=(_talk,lastTalkHistory)
	for _i=0;_i<10;_i++{
		_lasttalk[_i]=lastTalkHistory[_i]
	}
	lastTalkHistory=_lasttalk
}
//---- user與人格對話 ----------------------------------------------------------------------
ReplyToUser{
	if chatroom==1{
		reference1=REPLACE(reference1,'|','CHR(124)')
		_usertime="%(hour):%(minute):%(second)"
		lastTalk = TalkToUser
		_lastTalk=REPLACE(lastTalk,'|','CHR(124)')
		_selftime="%(hour):%(minute):%(second)"
		if '\1' !_in_ _lastTalk{
			_lastTalk=RE_REPLACE(_lastTalk,'\.*','')
			chatroomtxt+="\0%(username) %(_usertime)\n%(reference1)\n|"
			chatroomtxt+="\0%(selfname) %(_selftime)\n"
		}
		else{
			if '\0' _in_ _lastTalk{
				chatroomtxt+="\0%(username) %(_usertime)\n%(reference1)\n|"
				chatroomtxt+="\0%(selfname) %(_selftime)\n"
			}
			else
				chatroomtxt+="\1%(username) %(_usertime)\n%(reference1)\n|"
			_lastTalk=REPLACE(_lastTalk,'\1',"\1%keroname %(_selftime)\n")
			_lastTalk=RE_REPLACE(_lastTalk,'\.*','')
		}
		if callingtulingapi{
			ERASEVAR('callingtulingapi')
			lastTalk
		}
		else{
			chatroomtxt+="%(_lastTalk)#"
			OnChatRoom(j)+WriteChatroom
			--
			if voice
				TxtToVoice(lastTalk,voicename)
		}
	}
	else{
		lastTalk = TalkToUser
		lastTalk
	}
}
//---- user與人格對話 ----------------------------------------------------------------------
TalkToUser{
	kikkamood+=5
	if KikkaTalkmode==1
		KikkaTalkToUser
	else {
		scope = '2'
		ONE
		--
		TWO
	}
}
//**** ONE *************************************************
ONE{
	if !ISVAR('scope')
		scope = '0'
	//呼叫名字
	if reference1 == '橘花'{
		CallKikka
	}
	elseif reference1 == '鬥和'{
		CallTowa
	}
	//---- 2並-2字 ----
	elseif '橘花' _in_ reference1 && '鬥和' _in_ reference1{
		''
	}
	//---- 2並-1字 ----
	elseif '我' _in_ reference1 && '你' _in_ reference1{//“我”沒用的話這句也不用
		scope = '0'
		''
	}
	//---- 1-2字 ----
	elseif '鬥和' _in_ reference1{
		scope = '1'
	}
	elseif '我' _in_ reference1{
		scope = '0'
		"%(username)\w9\w9\n"
		''
		''
	}
	elseif '你' _in_ reference1 || '橘花' _in_ reference1{
		scope = '0'
	}
	else{
		scope = '0'
		''
	}
}
//**** TWO *************************************************
TWO{
	_t = ''
	if _t = TWO.commnd && !InAIGhostReply
		_t
	elseif !KikkaAImode {
		if _t = TWO.aitalk
			_t
	}
	if !_t{
		if _t = TWO.dicpart
			_t
		elseif KikkaAImode {
			if _t = TWO.aitalk
				_t
		}
		if !_t
			TWO.others
	}
}
TWO.commnd{
	reference1 = TOSIMPLIFIED(reference.raw[1])
	reference1 = RE_REPLACE(reference1,'/\s*teach\s*$','/teach')
	reference1 = RE_REPLACE(reference1,'/\s*talk\s*$','/talk')
	if '/teach' == reference1{
		Teach
	}
	elseif '/talk' == reference1{
		aimode=''
		'\0\s[5]進入正常模式'
	}
	elseif aimode=='teach'{
		AITeach(reference1)
	}
}
TWO.aitalk{
	AIGet(reference1)
}
TWO.dicpart{
	_timeslot = GetTimeSlot
	//----------------------------------------橘花自動------------------------------------------------------------------
	if TOINT(Tulingapi[0])==0&&TOINT(Tulingapi[1])==1||'查一下' _in_ reference1||'查一查' _in_ reference1||'查查' _in_ reference1
		OntulingApi(reference1)
	elseif '日曆' _in_ reference1{
		"\![open,calendar]\w9\w9\0\s[6]今天是%(year)年%(month)月%(day)日."
	}
	elseif '今天' _in_ reference1 &&('幾日' _in_ reference1||'幾號' _in_ reference1||'幾月' _in_ reference1||'幾年' _in_ reference1){
		"\0\s[6]今天是%(year)年%(month)月%(day)日."
	}
	elseif '打開' _in_ reference1{
		_n=reference1[1,'打開']
		_r=OnCallFunction(_n)
		if _r!=''{
			if 'Select.' _in_ _r
				_r=EVAL(_r)
			elseif 'On' _in_ _r
				_r='\![raise,'+_r+']'
			"\0\s[5]好的,打開%(_n)\x%(_r)"
		}
	}
	elseif (_t=OnPoemAnswer(reference1))!=''{
		_t
	}
	elseif RE_GREP(reference1,'被\S*?欺負'){
		_name=RE_REPLACEEX(reference1,'被(\S*?)欺負','$1')
		_name = TOLOWER(TOSIMPLIFIED(_name))
		if 'towa' _in_ _name || '鬥和' _in_ _name
			Select.towabully
		elseif '老師' _in_ _name
			Select.teacherbully
		elseif '友人' _in_ _name || '朋友' _in_ _name || '網友' _in_ _name || '損友' _in_ _name || '宅友' _in_ _name//nmd，直接'友' _in_ _name又會有這種叫張學友的人
			Select.friendsbully
		else
			Select.bully
	}
	//--------------------------------橘花AI對話系統--------------------------------
	elseif '多少句' _in_ reference1 || '多少對話' _in_ reference1 || '對話' _in_ reference1
		'橘花學會了超過'+AILoad(reference1,'qnum')+'句對話哦'
